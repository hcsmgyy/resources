rityBasic\", \"linkF,linkF1\"],\r\n  [\"SecurityAdvanced\", \"linkF,linkF2\"],\r\n  [\"SettingBasic\", \"linkG,linkG1\"],\r\n  [\"SettingTime\", \"linkG,linkG2\"],\r\n  [\"SettingDisplaySetting\", \"linkG,linkG3\"],\r\n  [\"SettingVoiceAssistant\", \"linkG,linkG4\"],\r\n  [\"SettingAudio\", \"linkG,linkG5\"],\r\n  [\"ContactsLocalContacts\", \"linkH,linkH1\"],\r\n  [\"ContactsCallLog\", \"linkH,linkH2\"],\r\n  [\"ArmingZoneSetting\", \"linkI,linkI1\"],\r\n  [\"ArmingMode\", \"linkI,linkI2\"],\r\n  [\"ArmingDisarmCode\", \"linkI,linkI3\"],\r\n  [\"ArmingAlarmAction\", \"linkI,linkI4\"],\r\n]);\r\n\r\nexport default {\r\n  name: \"Left\",\r\n  components: {\r\n    MenuArrow,\r\n  },\r\n  data() {\r\n    return {\r\n      rootSubmenuKeys: [\r\n        \"linkA\",\r\n        \"linkB\",\r\n        \"linkC\",\r\n        \"linkD\",\r\n        \"linkE\",\r\n        \"linkF\",\r\n        \"linkG\",\r\n        \"linkH\",\r\n        \"linkI\",\r\n      ],\r\n      openKeys: [\"linkA\"],\r\n      defaultSelectKeys: [\"linkA1\"],\r\n      selectKeys: [\"linkA1\"],\r\n    };\r\n  },\r\n  methods: {\r\n    onOpenChange(openKeys) {\r\n      const latestOpenKey = openKeys.find(\r\n        (key) => this.openKeys.indexOf(key) === -1\r\n      );\r\n      if (this.rootSubmenuKeys.indexOf(latestOpenKey) === -1) {\r\n        this.openKeys = openKeys;\r\n      } else {\r\n        this.openKeys = latestOpenKey ? [latestOpenKey] : [];\r\n      }\r\n      if (openKeys.length != 0) {\r\n        console.log(openKeys[openKeys.length - 1], \"open\");\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    $route(to, from) {\r\n      console.log(\"watch\");\r\n      //浏览器点后退的时候，选中地区没法自动重新选中，需要类似这种方式来实现\r\n      if (linkMap.has(to.name)) {\r\n        let link = linkMap.get(to.name).split(\",\");\r\n        this.openKeys = [link[0]];\r\n        this.selectKeys = [link[1]];\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    if (linkMap.has(this.$route.name)) {\r\n      let link = linkMap.get(this.$route.name).split(\",\");\r\n      this.openKeys = [link[0]];\r\n      this.selectKeys = [link[1]];\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.logo {\r\n  height: 90px;\r\n  width: 350px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.logo img {\r\n  width: 163px;\r\n  height: 49px;\r\n  /* margin-top: 20px;\r\n  margin-left: 40px; */\r\n}\r\n\r\n.logo label {\r\n  font-size: 32px;\r\n  color: white;\r\n  font-weight: bold;\r\n  border-left-style: solid;\r\n  margin-left: 19px;\r\n  padding-left: 14px;\r\n}\r\n\r\n.ant-layout-sider-children {\r\n  background: #252834;\r\n}\r\n.ant-menu-submenu-open,\r\n.ant-menu-sub {\r\n  background: #20242e !important;\r\n}\r\n.ant-menu-inline .ant-menu-item:not(:last-child),\r\n.ant-menu-inline .ant-menu-submenu-title,\r\n.ant-menu-inline .ant-menu-item {\r\n  margin-bottom: 0px !important;\r\n  margin-top: 0px !important;\r\n}\r\n.ant-menu-sub.ant-menu-inline > .ant-menu-item,\r\n.ant-menu-inline > .ant-menu-submenu > .ant-menu-submenu-title {\r\n  height: 65px !important;\r\n  line-height: 65px !important;\r\n}\r\n\r\n.ant-menu-inline .ant-menu-submenu-title,\r\n.ant-menu-inline .ant-menu-item {\r\n  font-size: 20px !important;\r\n}\r\n\r\n.ant-menu-submenu-title .anticon + span {\r\n  padding-left: 10px;\r\n}\r\n\r\n.ant-menu-submenu-open > div {\r\n  border-left-style: solid;\r\n  border-color: #0091ea;\r\n  border-left-width: 5px;\r\n}\r\n\r\n.ant-menu-item {\r\n  display: list-item !important;\r\n  padding-left: 70px !important;\r\n  list-style-type: circle !important;\r\n}\r\n\r\n.ant-menu-item-selected,\r\n.ant-menu-item-active {\r\n  list-style-type: disc !important;\r\n}\r\n\r\n.ant-menu-item > a {\r\n  display: unset !important;\r\n}\r\n\r\n.ant-menu-submenu-arrow {\r\n  display: none;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}