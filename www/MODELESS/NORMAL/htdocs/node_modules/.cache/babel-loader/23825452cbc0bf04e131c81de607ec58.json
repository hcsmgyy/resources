         <MenuArrow></MenuArrow>\r\n        </span>\r\n        <a-menu-item key=\"linkF1\">\r\n          <router-link to=\"SecurityBasic\">{{ $t(\"Basic\") }}</router-link>\r\n        </a-menu-item>\r\n        <a-menu-item key=\"linkF2\">\r\n          <router-link to=\"SecurityAdvanced\">{{ $t(\"Advance\") }}</router-link>\r\n        </a-menu-item>\r\n      </a-sub-menu>\r\n      <a-sub-menu key=\"linkG\">\r\n        <span slot=\"title\">\r\n          <my-icon type=\"iconsetting\" :style=\"{ fontSize: '25px'}\"></my-icon>\r\n          <span>{{ $t(\"Settings\") }}</span>\r\n          <MenuArrow></MenuArrow>\r\n        </span>\r\n        <a-menu-item key=\"linkG1\">\r\n          <router-link to=\"SettingBasic\">{{ $t(\"Basic\") }}</router-link>\r\n        </a-menu-item>\r\n        <a-menu-item key=\"linkG2\">\r\n          <router-link to=\"SettingTime\">{{ $t(\"Time\") }}</router-link>\r\n        </a-menu-item>\r\n        <a-menu-item key=\"linkG3\">\r\n          <router-link to=\"SettingDisplaySetting\">{{ $t(\"DisplaySetting\") }}</router-link>\r\n        </a-menu-item>\r\n        <a-menu-item key=\"linkG4\">\r\n          <router-link to=\"SettingVoiceAssistant\">{{ $t(\"AVSName\") }}</router-link>\r\n        </a-menu-item>\r\n        <a-menu-item key=\"linkG5\">\r\n          <router-link to=\"SettingAudio\">{{ $t(\"Audio\") }}</router-link>\r\n        </a-menu-item>\r\n      </a-sub-menu>\r\n      <a-sub-menu key=\"linkH\">\r\n        <span slot=\"title\">\r\n          <my-icon type=\"iconContacts\" :style=\"{ fontSize: '25px'}\"></my-icon>\r\n          <span>{{ $t(\"Contacts\") }}</span>\r\n          <MenuArrow></MenuArrow>\r\n        </span>\r\n        <a-menu-item key=\"linkH1\">\r\n          <router-link to=\"ContactsLocalContacts\">{{ $t(\"LocalContacts\") }}</router-link>\r\n        </a-menu-item>\r\n        <a-menu-item key=\"linkH2\">\r\n          <router-link to=\"ContactsCallLog\">{{ $t(\"CallLog\") }}</router-link>\r\n        </a-menu-item>\r\n      </a-