A,MAAA,EAAA,gBAAA,KAAA,EAAA,GAAA,EAAA,MAAA,EAAA;AACA,WAAA,aAAA,CAAA,YAAA,CAAA,GAAA,GAAA,GAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,KAAA,EAAA,GAAA;AACA,WAAA,aAAA,CAAA,YAAA,CAAA,OAAA,GAAA,IAAA;AACA,KA/CA;AAgDA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,mBAAA,CAAA,eAAA,GAAA,EAAA;AACA;AAlDA,GAxBA;AA6EA,EAAA,QAAA,EAAA;AACA,IAAA,YAAA,EAAA,wBAAA;AACA,aAAA,KAAA,gBAAA,GAAA,KAAA,mBAAA,GAAA,IAAA;AACA;AAHA,GA7EA;AAkFA,EAAA,KAAA,EAAA;AACA,IAAA,eADA,2BACA,MADA,EACA,MADA,EACA;AACA,UAAA,KAAA,UAAA,EAAA;AACA,aAAA,eAAA,CAAA,cAAA,GAAA,MAAA,CAAA,MAAA,IAAA,CAAA;AACA;;AACA,WAAA,mBAAA,CAAA,eAAA,GAAA,MAAA;AACA,KANA;AAOA,IAAA,SAPA,qBAOA,MAPA,EAOA,MAPA,EAOA;AACA,UAAA,KAAA,UAAA,EAAA;AACA,aAAA,eAAA,CAAA,iBAAA,GAAA,MAAA,CAAA,MAAA,IAAA,CAAA;AACA;AACA;AAXA,GAlFA;AA+FA,EAAA,IA/FA,kBA+FA;AAAA;;AACA,WAAA;AACA,MAAA,gBAAA,EAAA;AACA,QAAA,QAAA,EAAA,MADA;AAEA,QAAA,SAAA,EAAA;AAFA,OADA;AAKA,MAAA,cAAA,EAAA;AACA,QAAA,QAAA,EAAA,MADA;AAEA,QAAA,SAAA,EAAA,QAFA;AAGA,QAAA,MAAA,EAAA,6BAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OALA;AAWA,MAAA,iBAAA,EAAA;AACA,QAAA,KAAA,EAAA;AADA,OAXA;AAcA,MAAA,eAAA,EAAA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,MAAA,EAAA,mBAFA;AAGA,QAAA,YAAA,EAAA,KAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAdA;AAoBA,MAAA,mBAAA,EAAA;AACA,QAAA,KAAA,EAAA,SADA;AAEA,QAAA,QAAA,EAAA;AAFA,OApBA;AAwBA,MAAA,qBAAA,EAAA;AACA,QAAA,KAAA,EAAA,SADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAxBA;AA4BA,MAAA,mBAAA,EAAA;AACA,QAAA,QAAA,EAAA,kBAAA,eAAA,EAAA,YAAA,EAAA;AACA,cAAA,MAAA,GAAA;AACA,YAAA,IAAA,EAAA,WADA;AAEA,YAAA,MAAA,EAAA,eAFA;AAGA,YAAA,MAAA,EAAA,EAHA;AAIA,YAAA,gBAAA,EAAA;AAJA,WAAA;;AAMA,UAAA,KAAA,CAAA,KAAA,CAAA,QAAA,EAAA,MAAA;AACA,SATA;AAUA,QAAA,eAAA,EAAA,KAAA;AAVA;AA5BA,KAAA;AAyCA;AAzIA,CAAA","sourcesContent":["<template>\n  <div class=\"ak-common-table\">\n    <CommonTableHead v-if=\"showHead\" :data=\"tableHeadData\" @onDataChange=\"onHeadDataChange($event)\"></CommonTableHead>\n    <a-table\n      :columns=\"tableColumns\"\n      :data-source=\"tableData\"\n      bordered\n      :rowSelection=\"rowSelection\"\n      :pagination=\"false\"\n    >\n      <template\n        v-for=\"(column,columnIndex) in tableColumns\"\n        :slot=\"column.dataIndex\"\n        slot-scope=\"text,record\"\n      >\n        <div :key=\"columnIndex\">\n          <!-- 输入框 -->\n          <div v-if=\"column.type=='input'\">\n            <a-tooltip\n              :trigger=\"text.isWarn ? 'hover' : 'none'\"\n              :getPopupContainer=\"triggerNode => {return triggerNode.parentNode;}\"\n            >\n              <span slot=\"title\" :style=\"{display:'flex'}\">\n                <MyIcon type=\"iconerror\" class=\"ak-tooltips-icon\"></MyIcon>\n                <span class=\"ak-tooltips-label\">{{text.warnLabel}}</span>\n              </span>\n              <div v-if=\"text.itemType == 'select'\">\n                <a-select\n                  :value=\"text.value.toString()\"\n                  :style=\"text.isWarn? warnSelectStyle : commonSelectStyle\"\n                  :disabled=\"text.disabled\"\n                  @change=\"e=>handleSelectChange(e, record.key, column)\"\n                  dropdownClassName=\"ak-select-item\"\n                >\n                  <a-icon\n                    slot=\"suffixIcon\"\n                    type=\"caret-down\"\n                    theme=\"filled\"\n                    :style=\"text.isWarn? warnSelectIconStyle : commonSelectIconStyle\"\n                  />\n                  <a-select-option\n                    v-for=\"item in column.selectItems\"\n                    :key=\"item.key.toString()\"\n                  >{{ item.label }}</a-select-option>\n                </a-select>\n              </div>\n              <div v-else>\n                <a-input\n                  :type=\"column.inputType\"\n                  :value=\"text.value\"\n                  :maxLength=\"column.maxLength ? column.maxLength : 63\"\n                  @change=\"e => handleInputChange(e.target.value, record.key, column)\"\n                  :style=\"text.isWarn? warnInputStyle : commonInputStyle\"\n                  :disabled=\"text.disabled\"\n                  :addon-before=\"column.addOnBefore\"\n                ></a-input>\n              </div>\n            </a-tooltip>\n          </div>\n          <!-- 下拉选择框 -->\n          <div v-else-if=\"column.type=='select'\">\n            <a-tooltip\n              :trigger=\"text.isWarn ? 'hover' : 'none'\"\n              :getPopupContainer=\"triggerNode => {return triggerNode.parentNode;}\"\n            >\n              <span slot=\"title\" :style=\"{display:'flex'}\">\n                <MyIcon type=\"iconerror\" class=\"ak-tooltips-icon\"></MyIcon>\n                <span class=\"ak-tooltips-label\">{{text.warnLabel}}</span>\n              </span>\n              <div v-if=\"text.itemType == 'input'\">\n                <a-input\n                  :value=\"text.value\"\n                  :maxLength=\"column.maxLength ? column.maxLength : 63\"\n                  @change=\"e => handleInputChange(e.target.value, record.key, column)\"\n                  :style=\"text.isWarn? warnInputStyle : commonInputStyle\"\n                  :disabled=\"text.disabled\"\n                />\n              </div>\n              <div v-else>\n                <a-select\n                  :value=\"typeof text.value === 'number'?text.value.toString():text.value \"\n                  :style=\"text.isWarn? warnSelectStyle : commonSelectStyle\"\n                  :disabled=\"text.disabled\"\n                  :mode=\"column.mode\"\n                  @change=\"e=>handleSelectChange(e, record.key, column)\"\n                  dropdownClassName=\"ak-select-item\"\n                >\n                  <a-icon\n                    slot=\"suffixIcon\"\n                    type=\"caret-down\"\n                    theme=\"filled\"\n                    :style=\"text.isWarn? warnSelectIconStyle : commonSelectIconStyle\"\n                  />\n                  <a-select-option\n                    v-for=\"item in column.selectItems\"\n                    :key=\"item.key.toString()\"\n                  >{{ item.label }}</a-select-option>\n                </a-select>\n              </div>\n            </a-tooltip>\n          </div>\n          <!-- file input -->\n          <TableFileInput\n            v-else-if=\"column.type=='file-input'\"\n            :btnName=\"column.btnName\"\n            :value=\"text\"\n            :acceptFileType=\"column.acceptFileType\"\n            :upload=\"column.upload\"\n            :index=\"record.key\"\n            :isShow=\"text.isShow\"\n            @change=\"handleFileInputChange($event,record.key, column)\"\n          ></TableFileInput>\n          <!-- checkbox -->\n          <a-checkbox\n            v-else-if=\"column.type=='checkbox'\"\n            :checked=\"text.value\"\n            :disabled=\"text.disabled\"\n            @change=\"e => handleCheckboxChange(e.target.checked,record.key, column)\"\n          ></a-checkbox>\n          <!-- edit图标 -->\n          <div v-else-if=\"column.type=='edit'\">\n            <MyIcon\n              type=\"iconEdit\"\n              :style=\"{cursor:'pointer',fontSize:'22px',color:'#0087F7'}\"\n              @click=\"onEdit(text,record.key,column)\"\n            ></MyIcon>\n          </div>\n          <!-- 常规label -->\n          <label v-else>{{text}}</label>\n        </div>\n      </template>\n    </a-table>\n    <CommonTableFooter v-if=\"showFooter\" :data=\"tableFooterData\" @onPageChange=\"clearSelect\"></CommonTableFooter>\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport { Icon } from \"ant-design-vue\";\nimport iconFront from \"../js/iconfont\";\nimport { getTransTitle } from \"@/js/common.js\";\nimport CommonTableHead from \"@/components/CommonTableHead.vue\";\nimport CommonTableFooter from \"@/components/CommonTableFooter.vue\";\nimport TableFileInput from \"@/components/TableFileInput.vue\";\n\nconst IconFont = Icon.createFromIconfontCN({\n  scriptUrl: iconFront,\n});\n\nVue.component(\"MyIcon\", IconFont);\n\nexport default {\n  components: {\n    CommonTableHead,\n    CommonTableFooter,\n    TableFileInput,\n  },\n  props: {\n    showHead: { type: Boolean, default: false },\n    showFooter: { type: Boolean, default: false },\n    tableColumns: Array,\n    tableData: Array,\n    showRowSelection: {\n      type: Boolean,\n      default: false,\n    },\n    tableHeadData: Object,\n    tableFooterData: Object,\n    selectedRowKeys: {\n      type: Array,\n      default: function () {\n        return [];\n      },\n    },\n  },\n  methods: {\n    handleInputChange: function (value, key, column) {\n      //隐藏tips，待优化，这个写法不好\n      let tipsArr = document.getElementsByClassName(\"ant-tooltip\");\n      for (let i = 0; i < tipsArr.length; i++) {\n        tipsArr[i].style.display = \"none\";\n      }\n      this.onChange(value, key, column.dataIndex, \"input\");\n    },\n    handleCheckboxChange: function (checked, key, column) {\n      this.onChange(checked, key, column.dataIndex, \"checkbox\");\n    },\n    handleFileInputChange: function (event, key, column) {\n      this.onChange(event, key, column.dataIndex, \"file-input\");\n    },\n    handleSelectChange: function (value, key, column) {\n      //隐藏tips，待优化，这个写法不好\n      let tipsArr = document.getElementsByClassName(\"ant-tooltip\");\n      for (let i = 0; i < tipsArr.length; i++) {\n        tipsArr[i].style.display = \"none\";\n      }\n      this.onChange(value, key, column.dataIndex, \"select\");\n    },\n    onChange: function (value, index, dataIndex, type) {\n      let data = {\n        value: value,\n        index: index,\n        dataIndex: dataIndex,\n        type: type,\n      };\n      this.$emit(\"change\", data);\n    },\n    onHeadDataChange: function (event) {\n      let result = {\n        type: \"modal\",\n        oldVal: event.oldVal,\n        newVal: event.newVal,\n        modalKey: event.modalKey,\n        modalIndex: event.modalIndex,\n        tableOperateType: event.type,\n      };\n      this.$emit(\"change\", result);\n    },\n    onEdit: function (index, key, column) {\n      this.tableHeadData.addModalData.key = key;\n      column.onEdit(index, key);\n      this.tableHeadData.addModalData.visible = true;\n    },\n    clearSelect: function () {\n      this.defaultRowSelection.selectedRowKeys = [];\n    },\n  },\n\n  computed: {\n    rowSelection: function () {\n      return this.showRowSelection ? this.defaultRowSelection : null;\n    },\n  },\n  watch: {\n    selectedRowKeys(newVal, oldVal) {\n      if (this.showFooter) {\n        this.tableFooterData.deleteDisabled = newVal.length <= 0;\n      }\n      this.defaultRowSelection.selectedRowKeys = newVal;\n    },\n    tableData(newVal, oldVal) {\n      if (this.showFooter) {\n        this.tableFooterData.deleteAllDisabled = newVal.length <= 0;\n      }\n    },\n  },\n  data() {\n    return {\n      commonInputStyle: {\n        fontSize: \"16px\",\n        textAlign: \"center\",\n      },\n      warnInputStyle: {\n        fontSize: \"16px\",\n        textAlign: \"center\",\n        border: \"1px solid rgba(255,86,96,1)\",\n        color: \"#FF5660\",\n      },\n      commonSelectStyle: {\n        width: \"100%\",\n      },\n      warnSelectStyle: {\n        width: \"100%\",\n        border: \"1px solid #F29293\",\n        borderRadius: \"4px\",\n        color: \"#F29293\",\n      },\n      warnSelectIconStyle: {\n        color: \"#F29293\",\n        fontSize: \"20px\",\n      },\n      commonSelectIconStyle: {\n        color: \"#666666\",\n        fontSize: \"20px\",\n      },\n      defaultRowSelection: {\n        onChange: (selectedRowKeys, selectedRows) => {\n          let result = {\n            type: \"selection\",\n            newVal: selectedRowKeys,\n            oldVal: \"\",\n            tableOperateType: \"selection\",\n          };\n          this.$emit(\"change\", result);\n        },\n        selectedRowKeys: this.selectedRowKeys,\n      },\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.ak-common-table {\n  margin-left: 35px;\n  margin-right: 35px;\n  margin-top: 20px;\n}\n\n/deep/.ant-table-thead {\n  background: #0088f7;\n}\n/deep/.ant-table {\n  font-size: 16px;\n}\n\n/deep/.ant-table-thead > tr > th {\n  background: rgba(255, 255, 255, 0.9);\n  font-weight: bold;\n}\n/deep/.ant-table-tbody > tr > td {\n  padding-left: 6px;\n  padding-right: 6px;\n  padding-top: 8px;\n  padding-bottom: 8px;\n}\n\n.ak-tooltips-icon {\n  font-size: 16px;\n  color: #f75f61;\n  align-items: center;\n  justify-content: center;\n  display: flex;\n}\n\n.ak-tooltips-label {\n  max-width: 340px;\n  text-indent: 5px;\n  margin-left: 10px;\n}\n\n/deep/.ant-tooltip-arrow {\n  display: none;\n}\n\n/deep/.ant-tooltip {\n  max-width: 380px;\n}\n/deep/.ant-tooltip-inner {\n  background: rgba(251, 239, 239, 1);\n  color: #656565;\n  font-size: 14px;\n  border: 1px solid rgba(242, 146, 147, 1);\n  border-radius: 3px;\n}\n\n/deep/.ant-tooltip-placement-top {\n  padding-bottom: 3px;\n}\n\n.ant-table-wrapper {\n  pointer-events: none;\n}\n/deep/.ant-table {\n  pointer-events: auto;\n}\n\n/deep/.ant-select-arrow {\n  top: 40%;\n}\n\n/deep/.ant-select-selection {\n  text-align: center;\n}\n/deep/.ant-select-selection-selected-value {\n  float: left;\n  font-size: 16px;\n  width: 100%;\n}\n</style>"],"sourceRoot":"src/components"}]}