 = {\n        target: \"login\",\n        action: \"logout\",\n      };\n      this.$axios.post(\"api\", requestData).then((response) => {\n        delCookie(\"token\");\n        this.$router.push(\"/\");\n      });\n    },\n  },\n};\n</script>\n\n\n<style lang=\"less\" scoped>\n.ak-top {\n  height: 100%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  float: right;\n}\n\n/deep/.ant-select-selection {\n  border-radius: 22px;\n}\n/deep/.ant-select-selection__rendered {\n  margin-left: 20px;\n}\n/deep/.ant-select-arrow {\n  top: 40%;\n}\n</style>\n"],"sourceRoot":"src/components"}]}